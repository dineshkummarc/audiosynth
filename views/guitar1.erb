<!-- ADSR -->

<div id="adsr" class="box">
   <canvas id="adsr_graph" width="200" height="100"></canvas>
  <div class="on_off">
    <p class="field switch">
   	<label class="cb-enable"><span>On</span></label>
   	<label class="cb-disable selected"><span>Off</span></label>
   	<input type="checkbox" class="checkbox hide adsr_on" name="adsr_on" checked="false" />
   </p>
 </div>

  <h1>ADSR</h1>
   
   
  <div class="s"><label>A </label> <input name="adsr_a" id="adsr_a" type="text" title="Attack Time" value="0.01"  maxlength="6" onchange="alert('ddd')"/> </div>
  <div class="s"><label>D </label><input name="adsr_d" id="adsr_d" type="text" title="Decay Time" value="0.3"  maxlength="6" /></div>
  <div class="s"><label>S </label><input name="adsr_s_level" id="adsr_s_level" type="text" title="Sustain Level" value="0.4"  maxlength="6" /></div>
  <div class="s"><label>R </label><input name="adsr_r" id="adsr_r" type="text" title="Release Time" value="0.7"  maxlength="6" /></div>
  <input name="adsr_s" id="adsr_s" type="hidden" title="Sustain Time" value="0.3"  maxlength="6" />
  

</div><!-- eo ADSR-->






<div class="lfo box" id="lfo1">
   <!-- VOL SLIDER -->
    <div class="lfo_volume">
      <input name="lfo_vol" id="lfo_vol" type="text" title="LFO Amplitude" value="1"  maxlength="6"  />
     <!-- <label for="lfo_vol" id="lfo_vol_label">Amp</label>--> 
      <br>
    </div>
    
    <!-- ON / OFF -->
    <div class="on_off">
    <p class="field switch">
    	<label class="cb-enable"><span>On</span></label>
    	<label class="cb-disable selected"><span>Off</span></label>
    	<input type="checkbox" class="checkbox hide lfo_on" name="lfo_on" data-lfo="lfo1" checked="false" />
    </p>
    </div>
    <h1>LFO</h1>
    
    <div class="clear-left"></div>
    <!-- FREQ SLIDER -->
    <div class="s">
      <label><em>Rate </em>(osciliation freq.) </label>
      <input name="lfo_freq" id="lfo_freq" class="lfo_freq" type="text" title="Frequency" value="-18"  maxlength="6" />
    </div>
  
    <!-- freq display-->
    <p class="right">
    <em> <span class="actual_freq">440</span>mhz </em> <span class="note_name"></span>
    
    </p>
    
  <!-- chkbox even-->
  <label for="chk_even">
    <input type="checkbox" class="chk_even" name="chk_even" checked="true" />
    Even frequencies only
  </label>
  </div>
</div><!-- eo LFO -->













<input name="master_volume" id="master_volume" type="hidden" title="Master Volume" value="1"  maxlength="6"  />



</div> <!-- eo #efects-->



    
   
   
 <div id="sine_graph_wrap">
   
    <canvas id="sine_graph" width="1200" height="200"></canvas>
    <small>
       note waveform.<br/> 
       <span class="actual_note_duration"> </span> sec
      </small>
     
  </div>




   
   
   
   
